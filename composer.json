{
    "name": "laravel-admin-ext/messages",
    "description": "Message extension for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "messages"],
    "homepage": "https://github.com/laravel-admin-extensions/messages",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/laravel": "^6.0",
        "encore/laravel-admin": "~1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
       "laravel/laravel": "^6.0",
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\Message\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\Message\\MessageServiceProvider"
            ]
        }
    }
}
